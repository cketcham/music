<h1>Editing song</h1>

<% form_for(@song) do |f| %>
  <%= f.error_messages %>
<% #maybe this will turn into a remove? %>
	<div class="song left">
	  <ul>
		<li>
	    <%= f.label 'Name' %><br />
	    <%= f.text_field :name %>
		</li>
		<li>
	    <%= f.label 'Artist' %><br />
	    <%= f.text_field :artist_name %>
		</li>
		<li>
	    <%= f.label 'Album' %><br />
	    <%= f.text_field :album_title %>
		</li>
		<li>
		<%= f.submit "Update" %>
		</li>
	  </ul>
	</div>

    

<% end %>

<%= link_to 'Show', @song %> |
<%= link_to 'Back', songs_path %>
