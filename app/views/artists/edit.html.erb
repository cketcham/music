<h1>Editing artist</h1>

<% form_for(@artist) do |f| %>
  <%= f.error_messages %>
	<ul>
	  <li>
	    <%= f.label :name %><br />
	    <%= f.text_field :name %>
	  </li>
	  <li>
	    <%= f.submit "Update" %>
	  </li>
	</ul>
<% end %>

<%= link_to 'Show', @artist %> |
<%= link_to 'Back', artists_path %>
